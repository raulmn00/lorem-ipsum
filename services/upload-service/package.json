    "@nestjs/core": "^10.3.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "exifr": "^7.1.3",
    "file-type": "^16.5.4",
    "node-vibrant": "^3.1.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.1",
    "axios": "^1.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
